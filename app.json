{
  "expo": {
    "name": "StellarIQ",
    "slug": "stellariq",
    "sdkVersion": "54.0.0",
    "platforms": ["android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon-1024-1024.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "../assets/splash-4096-4096.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a3a5a"
    },
    "android": {
      "package": "com.stellariq.app",
      "versionCode": 1,
      "backgroundColor": "#1a3a5a",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/android/adaptive-icon/foreground.png",
        "backgroundImage": "./assets/android/adaptive-icon/background.png"
      }
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "color": "#ffffff"
        },
        "expo-splash-screen",
        {
          "android": {
            "backgroundColor": "#1a3a5a",
            "image": "./assets/splash-4096-4096.png",
            "resizeMode": "contain"
          }
        }
      ],
      "expo-font"
    ],
    "scheme": "stellariq",
    "extra": {
      "apiUrl": "http://192.168.1.74:8000",
      "eas": {
        "projectId": "2f38aa9b-5f7a-497e-bfad-3bd8e3ecdaaa"
      }
    }
  }
}
